{
  "name": "QuickGPT",
  "icon": "iconify:logos:openai-icon",
  "identifier": "dev.dyptan.popclip.extension.quickqpt",
  "description": "QuickGPT is an extension for PopClip that integrates OpenAI's powerful language models directly into your MacOS experience",
  "entitlements": ["network"],
  "requirements": ["text", "paste"],
  "excludedApps": ["com.apple.Terminal"],
  "macosVersion": "14.0",
  "popclipVersion": 4225,
  "module": "chatgpt.js",
  "options": [
    {
      "identifier": "apikey",
      "label": "API Key",
      "type": "string",
      "description": "Generate an API key at https://platform.openai.com/account/api-keys"
    },
    {
      "identifier": "model",
      "label": "Model",
      "type": "multiple",
      "defaultValue": "gpt-3.5-turbo",
      "description": "Specify the OpenAI model to use",
      "values": ["gpt-4", "gpt-3.5-turbo"]
    }
  ]
}